  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --open --hot --progress --mode development --devtool eval-source-map",
    "build": "webpack --env production --mode production --profile --progress",
    "postbuild": "make sitemap && react-snap"
  },
  "reactSnap": {
    "inlineCss": false,
    "source": "./docs",
    "removeStyleTags": false,
    "preloadImages": true,
    "asyncScriptTags": false,
    "removeScriptTags": false,
    "include": [
      "/index.html",
      "/admin/index.html"
    ],
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "destination": "./docs"
  },
  "dependencies": {
