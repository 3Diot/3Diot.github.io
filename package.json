{
  "name": "name",
  "description": "DESCRIPTION",
  "author": "Charles Karpati",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --open --hot --progress --mode development --devtool eval-source-map",
    "build": "webpack --mode production --profile --progress",
    "postbuild": "make sitemap && react-snap"
  },
  "reactSnap": {
    "puppeteerExecutablePath": "../latest_chromium/chrome.exe",
    "inlineCss": false,
    "source": "./docs",
    "removeStyleTags": false,
    "preloadImages": false,
    "asyncScriptTags": false,
    "removeScriptTags": false,
    "include": [
      "/index.html",
      "/admin/index.html"
    ],
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "destination": "./docs"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.5%"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.0.1",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-webpack-inline-source-plugin-patched": "^1.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "react-helmet-async": "^1.3.0",
    "react-snap": "^1.23.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0"
  }
}
