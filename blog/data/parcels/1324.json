]=5\n\n})\n\nb6.on('click', e=>{\n\n  position = code.findIndex(hasStarted)\n  code[position]=6\n\n})\n\nfunction arraysEqual(arr1,arr2){\n  i=arr1.length\n  for(i;i--;){\n    if(arr1[i] != arr2[i]){\n      return false;\n    }\n  }\n  return true;\n}\n\nfunction reset(){\n  code=[0,0,0,0]\n}\n\nfeature.on('click', e=>{\n  if(arraysEqual(correct,code)){\n    console.log(\"good\")  \n    txt.set({text:\"Granted\"})\n    door.set({rotation:[0,3.14,0]})\n   reset()\n  }else{\n\n    txt.set({text:\"Invalid\"})\n    reset()\n  }\n\n})", "color": "blue", "id": "enter"}, {"type": "sign", "scale": [0.5, 0.5, 0.5], "text": "", "position": [-11.75, 2.85, 0], "rotation": [0, 4.71238898038469, 0], "version": "5.27.5", "uuid": "37e71cec-be8f-490a-87c8-d1f1dba0b751", "animation": {"keyframes": []}, "script": "", "id": "correcttxt", "blendMode": "Multiply", "color": "#000000", "background": "#ffffff", "fontSize": "20"}, {"type": "button", "scale": [0.2, 0.15, 0.2], "text": "A", "position": [-11.75, 2.37, -0.15], "rotation": [1.57, 1.57, 0], "version": "5.31.2", "uuid": "b1d04008-4d6e-4995-811e-1d0e3875e28e", "script": "", "id": "b4", "soundId": "8"}, {"type": "vox-model", "scale": [4.9, 3.9, 1], "url": "https://wiki.cryptovoxels.com/simple_metal_door.vox", "flipX": true, "position": [-11.95, 0.75, 1.4], "rotation": [0, 1.57, 0], "version": "5.23.2", "uuid": "d0c97409-aa61-429b-a558-a076fa86c720", "animation": {"keyframes": []}, "script": "", "link": "", "collidable": true, "id": "door"}, {"type": "vox-model", "scale": [1.5, 0.5, 1.5], "url": "https://wiki.cryptovoxels.com/wooden_trap_door.vox", "flipX": true, "position": [-6.5, 0.75, -1], "rotation": [0, 1.5707963267948966, 0], "version": "5.22.0", "uuid": "30ecf63d-96f4-46e8-a38f-72b21ac301dd", "animation": {"keyframes": []}, "script": "", "id": "platform", "collidable": true}, {"type": "button", "scale": [0.5, 0.5, 0.5], "text": "A", "position": [-6.4, 0.75, -0.2], "rotation": [0, 1.5707963267948966, 0], "version": "5.61.3", "uu